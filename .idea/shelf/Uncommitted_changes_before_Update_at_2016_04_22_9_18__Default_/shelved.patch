+                    <option value="房租方式2">房租方式2</option>
+                </select>
+            </td>
+        </tr>
+        <tr>
+            <td>房价：</td>
+            <td><input type="text" value="100.00" disabled="disabled"  style="width: 120px;"></td>
+            <td>房数：</td>
+            <td>
+                <input type="button" class="jia reduceDays" value="-" style="margin-right: -1px;">
+                <input type="text" value="1" style="width: 60px;" id="Days">
+                <input type="button" class="jia addDays" value="+" style="margin-left: -1px;">
+                <span style="color: #666; font-size: 12px; margin-left: 10px;">可订 <font color="#5771d9">10</font>  间</span>
+            </td>
+            <td>网上单号：</td>
+            <td><input type="text" value="" style="width: 110px;"></td>
+        </tr>
+        <tr>
+            <td>来店时间：</td>
+            <td>
+                <input type="text" value="2016-05-22 10:00" style="width: 120px;">
+            </td>
+            <td>离店时间：</td>
+            <td>
+                <input type="text" value="2016-05-23 10:00" style="width: 110px;" class="datetimepicker_mask">
+            </td>
+            <td>留房时间：</td>
+            <td>
